{
    "name": "okvpn/expression-language",
    "description": "Fast powerful PHP expression language based on twig syntax",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/okvpn/expression-language",
    "support": {
        "email": "tsykun314@gmail.com",
        "issues": "https://github.com/okvpn/expression-language/issues",
        "source": "https://github.com/okvpn/expression-language/releases"
    },
    "keywords": [
        "expression-language",
        "safe-eval",
        "evaluate-expressions"
    ],
    "authors": [
        {
            "name": "Uladzimir Tsykun",
            "email": "tsykun314@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "twig/twig": "^2.7 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Okvpn\\Expression\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okvpn\\Expression\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
